msgid "Spellchecking and writing aids"
msgstr "Črkovalnik in pisalni pripomočki"

#: ../data/LanguageSelector.ui.h:28
msgid "Text"
msgstr "Besedilo"

#: ../data/LanguageSelector.ui.h:29
msgid "Translations"
msgstr "Prevodi"

#: ../data/LanguageSelector.ui.h:30
